<h2 class="title">New user</h2>

<div class="section">
  <%= simple_form_for @user do |f| %>
    <div class="field">
      <div class="control">
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.button :submit, 'Create new user'%>
      </div>
    </div>
    <% end %>
</div>
